{
  "name": "mac-setup",
  "version": "0.0.3",
  "homepage": "https://sourabhbajaj.com/mac-setup/",
  "description": "Installing Development environment on macOS",
  "private": true,
  "workspaces": [
    "website"
  ],
  "scripts": {
    "start": "npm run start --workspace=website",
    "build": "npm run build --workspace=website",
    "serve": "npm run serve --workspace=website",
    "lint": "markdownlint . --ignore node_modules --ignore website/node_modules --ignore website/build",
    "install-deps": "npm install"
  },
  "devDependencies": {
    "markdownlint-cli": "0.34.0",
    "node-fetch": "^3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sb2nov/mac-setup.git"
  },
  "keywords": [
    "OSx",
    "OS X",
    "Install",
    "Mac",
    "macOS",
    "Git",
    "Docusaurus",
    "Setup",
    "Instruction"
  ],
